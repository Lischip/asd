	journaltitle = {Journal of Food Safety},
	author = {Nastasijevic, Ivan and Proscia, Francesco and Boskovic, Marija and Glisic, Milica and Blagojevic, Bojan and Sorgentone, Simona and Kirbis, Andrej and Ferri, Maurizio},
