@article{feder_locomotion_2010,
	title = {Locomotion in Response to Shifting Climate Zones: Not So Fast},
	volume = {72},
	url = {https://doi.org/10.1146/annurev-physiol-021909-135804},
	doi = {10.1146/annurev-physiol-021909-135804},
	shorttitle = {Locomotion in Response to Shifting Climate Zones},
	abstract = {Although a species' locomotor capacity is suggestive of its ability to escape global climate change, such a suggestion is not necessarily straightforward. Species vary substantially in locomotor capacity, both ontogenetically and within/among populations, and much of this variation has a genetic basis. Accordingly, locomotor capacity can and does evolve rapidly, as selection experiments demonstrate. Importantly, even though this evolution of locomotor capacity may be rapid enough to escape changing climate, genetic correlations among traits (often due to pleiotropy) are such that successful or rapid dispersers are often limited in colonization or reproductive ability, which may be viewed as a trade-off. The nuanced assessment of this variation and evolution is reviewed for well-studied models: salmon, flying versus flightless insects, rodents undergoing experimental evolution, and metapopulations of butterflies. This work reveals how integration of physiology with population biology and functional genomics can be especially informative.},
	pages = {167--190},
	number = {1},
	journaltitle = {Annual Review of Physiology},
	author = {Feder, Martin E. and Garland, Theodore and Marden, James H. and Zera, Anthony J.},
	urldate = {2021-02-24},
	date = {2010},
	pmid = {20148672},
	note = {\_eprint: https://doi.org/10.1146/annurev-physiol-021909-135804},
	file = {Full Text:D\:\\zotero\\storage\\KJXX8Y9U\\Feder et al. - 2010 - Locomotion in Response to Shifting Climate Zones .pdf:application/pdf},
}

@article{sasaki_epidemiological_2000,
	title = {Epidemiological Potential of Excretion and Regurgitation by Musca domestica (Diptera: Muscidae) in the Dissemination of Escherichia coli O157: H7 to Food},
	volume = {37},
	issn = {00222585, 00222585},
	url = {https://academic.oup.com/jme/article-lookup/doi/10.1603/0022-2585-37.6.945},
	doi = {10.1603/0022-2585-37.6.945},
	shorttitle = {Epidemiological Potential of Excretion and Regurgitation by {\textless}I{\textgreater}Musca domestica{\textless}/I{\textgreater} (Diptera},
	abstract = {We previously reported that enterohemorrhagic Escherichia coli O157: H7 ({EHEC}) proliferates in the mouthparts of the house ßy Musca domestica vicina Macquart and are excreted for at least 3 d after ingestion. However, the role of the crop and excretory behavior of the house ßy in contamination of human food with {EHEC} is not known. In the current experiments, {EHEC} persisted in the crop of house ßies for at least 4 d. The number of {EHEC} in an excreted droplet was Ϸ104 1 h after bacterial feeding, Ͼ1.8 ϫ 105 3 h after feeding, and then drastically decreased after 24 h. Excretion is one of the major mechanisms for decreasing number of {EHEC} in the crop and gut of the house ßy. The frequency of excretion by females with developing eggs in their ovary was clearly higher (6.5 min per drop) than for males or females with mature eggs. Minute eosin-sign around a container Þlled with eosin-supplemented trypticase soy broth might be derived from frequent contact by house ßy contaminated mouthparts. These results show that frequent excretion potentially enhances the dissemination of {EHEC} to foods, particularly during the Þrst 24 h after ingestion of the bacteria.},
	pages = {945--949},
	number = {6},
	journaltitle = {Journal of Medical Entomology},
	shortjournal = {me},
	author = {Sasaki, Toshinori and Kobayashi, Mutsuo and Agui, Noriaki},
	urldate = {2021-02-24},
	date = {2000-11-01},
	langid = {english},
	file = {Sasaki et al. - 2000 - Epidemiological Potential of Excretion and Regurgi.pdf:D\:\\zotero\\storage\\4N5X69PU\\Sasaki et al. - 2000 - Epidemiological Potential of Excretion and Regurgi.pdf:application/pdf},
}

@article{huang_epidemiology_2013,
	title = {Epidemiology of Kawasaki Disease: Prevalence from National Database and Future Trends Projection by System Dynamics Modeling},
	volume = {163},
	issn = {00223476},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0022347612014576},
	doi = {10.1016/j.jpeds.2012.12.011},
	shorttitle = {Epidemiology of Kawasaki Disease},
	abstract = {Objective To update the epidemiologic trend in Kawasaki disease ({KD}) and develop models for projection. Study design From our national databases 2000-2010 and previous studies, we obtained the epidemiologic data to develop and validate system dynamics models. Population model incorporated birth rate and mortality. {KD} model incorporated the population at risk, incidence, and risk of coronary complications.
Results The average annual incidence in age group {\textless}5, 5-10, 10-15, and 15-20 years was 67.3, 5.75, 0.79, and 0.26 per 100 000. The {KD} population was 23 349 and the model estimated 20 254 patients with {KD}, and 25\% of these patients received medical care or continued surveillance in 2010. Projection up to 2030 suggests an average of 725 new patients with {KD} annually and a {KD} population of 35 006 by 2030. In 2030, 1469 patients with {KD} will need medical care for coronary complications. Simulation on the model modiﬁed to {US} data is also effective and suggests an average of 6200 new patients annually and {KD} population of 161 776 by 2030, and 5664 patients will need coronary care in 2030. By 2030, there will be 1 per 700 people in Taiwan and 1 per 1600 in the {US} with a history of {KD}.
Conclusion Simulations on our system dynamics models tailored to any epidemiologic and outcome variables and any changes with medical advance can dynamically project the futures. (J Pediatr 2013;163:126-31).},
	pages = {126--131.e1},
	number = {1},
	journaltitle = {The Journal of Pediatrics},
	shortjournal = {The Journal of Pediatrics},
	author = {Huang, San-Kuei and Lin, Min-Tai and Chen, Hui-Chi and Huang, Shin-Chung and Wu, Mei-Hwan},
	urldate = {2021-02-24},
