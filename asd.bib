@article{skovgard_retention_2011,
	title = {Retention of Campylobacter (Campylobacterales: Campylobacteraceae) in the House Fly (Diptera: Muscidae)},
	volume = {48},
	issn = {00222585, 00222585},
	url = {https://academic.oup.com/jme/article-lookup/doi/10.1603/ME11061},
	doi = {10.1603/ME11061},
	shorttitle = {Retention of Campylobacter (Campylobacterales},
	abstract = {The house ßy (Musca domestica L.) may transmit Campylobacter to broiler ßocks. We assessed the retention time of house ßies for Campylobacter jejuni at Þve temperatures and three doses. Flies were inoculated individually at their proboscis with 1.6 ϫ 107 {CFU} (colony forming units) of C. jejuni and incubated at 15, 20, 25, 30, and 35{ЊC}. Furthermore, a dose experiment was conducted at 25{ЊC} where individual ßies were inoculated in three series: 6.5 ϫ 106, 6.0 ϫ 104, and 8.2 ϫ 102 C. jejuni {CFU}. Whole ßies were tested for C. jejuni carriage at 0, 6, 12, 18, and 24 h by initial preenrichment in Bolton broth, which afterwards was streaked on {modiÞed} {mCCDA} agar plates and incubated under micro-aerobic conditions. The results showed that the time C. jejuni remained in ßies declined over time with ascending temperatures and when reducing the inoculation dose. All ßies stayed Campylobacter positive 24 h postinoculation at 15{ЊC} whereas only one-third of the ßies were positive at 20{ЊC} and few to none at 25, 30, and 35{ЊC}. When combinations of temperature and retention time were expressed as accumulated day-degrees, data could be adequately Þtted using a generalized linear mixed model that included a linear effect of day-degrees and the difference between the lowest and the two highest doses. Based on model predictions of selected combinations of temperature and dose, the time for 50\% and 1\% of ßies containing Campylobacter was calculated. It is suggested that house ßies are mainly short distance carriers of C. jejuni.},
	pages = {1202--1209},
	number = {6},
	journaltitle = {Journal of Medical Entomology},
	shortjournal = {jnl. med. entom.},
	author = {Skovgård, H. and Kristensen, K. and Hald, B.},
	urldate = {2021-02-24},
	date = {2011-11-01},
	langid = {english},
	file = {Skovgård et al. - 2011 - Retention of Campylobacter (Campylobacterales Cam.pdf:D\:\\zotero\\storage\\4QSDVZ59\\Skovgård et al. - 2011 - Retention of Campylobacter (Campylobacterales Cam.pdf:application/pdf},
